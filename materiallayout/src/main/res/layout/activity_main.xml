<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    tools:context=".MainActivity">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/coordinatorlayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CoordinatorLayout"
                android:textSize="24sp"/>
            <TextView

                android:id="@+id/with_viewpager"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="with_viewpager"
                android:textSize="24sp"/>
            <TextView
                android:id="@+id/collapsing_activity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="collapsing_activity"
                android:textSize="24sp"/>
            <TextView
                android:id="@+id/elema"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ElemaMap4DetailActivity"
                android:textSize="24sp"/>
            <TextView
                android:id="@+id/behavior_hide_activity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BehaviorHide"
                android:textSize="24sp"/>
            <TextView
                android:id="@+id/p6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hello World!"
                android:textSize="24sp"/>
        </LinearLayout>

    </android.support.v4.widget.NestedScrollView>



</LinearLayout>